programa {
  funcao inicio() {
 inteiro num1, num2 
 cadeia oper
  escreva("qual seu primeiro numero?")
  leia(num1)
  escreva("qual seu segundo numero?")
  leia(num2)
  escreva("qual sua operação aritmetica?")
  leia(oper)
  se (oper== "+")
  {escreva ("a soma eh ", num1+num2)}
  se (oper== "-")
  {escreva ("a subtracao eh ", num1-num2)}
  se (oper== "*")
  {escreva ("a subtracao eh ", num1*num2)}
  se (oper== "/")
  {escreva ("a divisao eh ", num1/num2)}
}
